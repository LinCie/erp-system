{
  "imports": {
    "@/": "./src/",
    "@google/genai": "npm:@google/genai@^1.31.0",
    "@hono/swagger-ui": "npm:@hono/swagger-ui@^0.5.1",
    "@hono/zod-openapi": "npm:@hono/zod-openapi@^1.1.5",
    "@std/crypto": "jsr:@std/crypto@^1.0.5",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@std/path": "jsr:@std/path@^1.1.3",
    "@std/text": "jsr:@std/text@^1.0.16",
    "@types/bcrypt": "npm:@types/bcrypt@^6.0.0",
    "argon2": "npm:argon2@^0.44.0",
    "bcrypt": "npm:bcrypt@^6.0.0",
    "hono": "npm:hono@^4.10.7",
    "kysely": "npm:kysely@^0.28.8",
    "mysql2": "npm:mysql2",
    "redis": "npm:redis@^5.10.0",
    "zod": "npm:zod@^4.1.13"
  },
  "tasks": {
    "start": "deno run --allow-net src/main.ts",
    "dev": "deno run --watch --allow-all --env-file ./src/main.ts",
    "create:module": "deno run --allow-write --allow-read scripts/create-module.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx",
    "strict": true
  },
  "nodeModulesDir": "auto",
  "allowScripts": true
}
